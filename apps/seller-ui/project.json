{
  "name": "seller-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/seller-ui",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "seller-ui:build",
        "dev": true,
        "port": 3001
      },
      "configurations": {
        "development": {
          "buildTarget": "seller-ui:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "seller-ui:build:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/seller-ui"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/seller-ui"
        },
        "production": {
          "outputPath": "dist/apps/seller-ui"
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "seller-ui:build",
        "dev": true,
        "port": 3001
      },
      "configurations": {
        "development": {
          "buildTarget": "seller-ui:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "seller-ui:build:production",
          "dev": false
        }
      }
    }
  },
  "tags": []
}
